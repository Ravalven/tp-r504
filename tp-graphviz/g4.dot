    H [pos="4,8!" image="router.png" label=""  xlabel="H"];

    A--B [label="1"];
    A--C [label="4"];
    B--C [label="2"];
    B--D [label="9"];
    B--G [label="4"];
    B--H [label="2"];
    C--D [label="1"];
    C--E [label="3"];
    D--E [label="1"];
